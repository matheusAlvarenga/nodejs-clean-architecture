{
    "env": {
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "rules": {
        "class-methods-use-this": "off",
        "consistent-return": "warn",
        "max-classes-per-file": "off"
    }
}
